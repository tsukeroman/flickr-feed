{
  "name": "flickr-feed",
  "version": "1.0.0",
  "description": "A web app that simuates a social network based on Flickr API",
  "main": "server.js",
  /*"scripts": {
    "start": "set DEBUG=app, set app:* & nodemon server.js",
    "test": "test"
  },*/
   "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsukeroman/flickr-feed.git"
  },
  "author": "tsukeroman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tsukeroman/flickr-feed/issues"
  },
  "homepage": "https://github.com/tsukeroman/flickr-feed#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.4",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "mongodb": "3.1.4",
    "morgan": "1.9.0",
    "nodemon": "1.18.4",
    "passport": "0.4.0",
    "passport-local": "1.0.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 5000
    }
  }
}
